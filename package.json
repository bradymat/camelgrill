{
  "name": "camelgrill",
  "version": "1.0.0",
  "description": "yoyo redux",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bradymat/camelgrill"
  },
  "author": "bradymat",
  "license": "Motosoto",
  "bugs": {
    "url": "https://github.com/bradymat/camelgrill/issues"
  },
  "homepage": "http://camelgrill.co.nz",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^9.2.2",
    "clone": "^2.1.1",
    "gh-pages": "^0.11.0",
    "redux": "^3.6.0",
    "sheet-router": "^4.1.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "yo-yo": "^1.3.1"
  }
}
